local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "shxdxwstxr", HidePremium = true, SaveConfig = false, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Jigoku Advanced",
	Icon = "rbxassetid://10566586189",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Return Back To Hub",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/hanakoesp/The-Mimic/the-mimic-extra/The%20Mimic%20Orion"))()
  	end    
})

Tab:AddButton({
	Name = "Auto Get Badge True",
	Callback = function()
      		-- config js -- 
_G.AutoGetBadge = true -- // false 
_G.NoLoading = false -- // false
_G.AutoFullbright = false -- // false
 loadstring(game:HttpGet("https://raw.githubusercontent.com/poisklmpoikslms901i234/-Trash/main/.lua"))()
  	end    
})

Tab:AddButton({
	Name = "Auto Get Badge, No Loading True",
	Callback = function()
      		-- config js -- 
_G.AutoGetBadge = true -- // false 
_G.NoLoading = true -- // false
_G.AutoFullbright = false -- // false
 loadstring(game:HttpGet("https://raw.githubusercontent.com/poisklmpoikslms901i234/-Trash/main/.lua"))()
  	end    
})

