 local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "shxdxwstxr", HidePremium = true, SaveConfig = false, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "KTollT",
	Icon = "rbxassetid://10566586189",
	PremiumOnly = false
})


Tab:AddButton({
	Name = "KTollT's Youtube Channel",
	Callback = function()
      		OrionLib:MakeNotification({
	Name = "Copied!",
	Content = "Copied KTollT's Youtube Channel",
	Image = "rbxassetid://10566586189",
	Time = 4
})

wait(0.4)
setclipboard("https://www.youtube.com/channel/UC7Qd6nnNiat95JWeIH0pX5Q/videos")
  	end    
})

